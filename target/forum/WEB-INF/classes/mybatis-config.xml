<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- 实体类,简称 -设置别名 -->
	<typeAliases>
		<!-- 将com.lcvc.ebuytest3.model.Admin类重新起一个名字Admin，供mybatis的映射文件使用 -->
		<typeAlias alias="Customer" type="com.bjbls.forum.model.Customer"/>
		<typeAlias alias="Admin"  type="com.bjbls.forum.model.Admin"/>
		<typeAlias alias="Send"  type="com.bjbls.forum.model.Send"/>
		<typeAlias alias="SendType"  type="com.bjbls.forum.model.SendType"/>
		<typeAlias alias="SendState"  type="com.bjbls.forum.model.SendState"/>
		<typeAlias alias="Page"  type="com.bjbls.forum.model.Page"/>
		<typeAlias alias="SendCus"  type="com.bjbls.forum.model.SendCus"/>
	</typeAliases>

	<mappers>
		<!--注册adminMappe.xml文件 -->
		<mapper resource="com/bjbls/forum/dao/mapper/CustomerMapper.xml"/>
		<mapper resource="com/bjbls/forum/dao/mapper/AdminMapper.xml"/>
	    <mapper resource="com/bjbls/forum/dao/mapper/SendMapper.xml"/>
		<mapper resource="com/bjbls/forum/dao/mapper/SendTypeMapper.xml"/>
		<mapper resource="com/bjbls/forum/dao/mapper/SendStateMapper.xml"/>
		<mapper resource="com/bjbls/forum/dao/mapper/PageMapper.xml"/>
		<mapper resource="com/bjbls/forum/dao/mapper/SendCusMapper.xml"/>

	</mappers>


</configuration>  